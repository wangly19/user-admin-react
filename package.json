    "@types/mockjs": "^1.0.3",
    "@umijs/plugin-antd": "^0.8.0",
    "@umijs/plugin-dva": "^0.8.2",
    "@umijs/plugin-initial-state": "^2.2.1",
    "@umijs/plugin-model": "^2.5.3",
    "@umijs/test": "^3.2.20",
    "add": "^2.0.6",
    "antd": "^4.6.3",
    "axios": "^0.20.0",
    "bizcharts": "^4.0.14",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.20",
    "umi-plugin-keep-alive": "^0.0.1-beta.16",
